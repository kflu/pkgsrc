$NetBSD: patch-xfsettingsd_xsettings.xml,v 1.6 2023/02/12 03:07:08 gutteridge Exp $

Use a good icon theme by default.

--- xfsettingsd/xsettings.xml.orig	2023-02-11 08:55:53.000000000 +0000
+++ xfsettingsd/xsettings.xml
@@ -6,8 +6,8 @@
 
 <channel name="xsettings" version="1.0">
   <property name="Net" type="empty">
-    <property name="ThemeName" type="empty"/>
-    <property name="IconThemeName" type="empty"/>
+    <property name="ThemeName" type="Adwaita"/>
+    <property name="IconThemeName" type="elementary-xfce"/>
     <property name="DoubleClickTime" type="int" value="400"/>
     <property name="DoubleClickDistance" type="int" value="5"/>
     <property name="DndDragThreshold" type="int" value="8"/>
